<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
    <shape xmlns:android="http://schemas.android.com/apk/res/android"
        android:shape="oval" >
        <stroke android:color="#444444"
            android:width="1dp"/>
        <solid
            android:color="#0d47a1"/>
        <size
            android:width="5dp"
            android:height="5dp"/>
    </shape>
    </item>
    <item
        android:id="@+id/icon_drawable"
        android:drawable="@drawable/ic_star_white"
        android:bottom="6dp"
        android:left="6dp"
        android:right="6dp"
        android:top="6dp"
        android:gravity="center"/>
</layer-list>